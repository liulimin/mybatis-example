<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mybatis.domain.Address">

	<resultMap type="Address" id="addressResultMap">
		<id column="addressId" property="addressId"/>
		<result column="addressName" property="addressName"/>
		<result column="postCode" property="postCode"/>
		<result column="addressee" property="addressee"/>
		<association property="user" column="userId2" javaType="User" select="org.example.mybatis.domain.User.selectUser"/>
	</resultMap>
	
	<select id="selectAddress" parameterType="string" resultMap="addressResultMap">
		select * from t_address where userId2 = #{userId}
	</select>
	
</mapper>